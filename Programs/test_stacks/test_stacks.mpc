one=regint(1)
two=regint(2)
three=regint(3)
four=regint(4)
regint.push(one)
regint.push(two)
sp=regint.getsp()
c=regint.peek(sp-1)
cc=regint.reverse_peek(regint(1))
regint.poke(sp,three)
regint.reverse_poke(regint(1),four)
d=regint.pop()
e=regint.pop()
test(c,one)
test(cc,one)
test(d,three)
test(e,four)
print_ln(' %s',c)
print_ln(' %s',d)
print_ln(' %s',e)

